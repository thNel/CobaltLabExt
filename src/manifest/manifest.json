{
  "manifest_version": 3,
  "name": "CobaltLab Helper",
  "version": "2.0.0.0",
  "action": {
    "default_icon": {
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "64": "images/icon64.png"
    },
    "default_title": "AutoRoulette",
    "default_popup": "index.html"
  },
  "description": "Автокликер для игры CobaltLab",
  "icons": {
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "64": "images/icon64.png",
    "80": "images/icon80.png",
    "96": "images/icon96.png",
    "128": "images/icon128.png",
    "160": "images/icon160.png"
  },
  "author": "thNel",
  "content_scripts": [
    {
      "matches": [
        "https://*.cobaltlab.tech/embedGame*"
      ],
      "all_frames": true,
      "css": [
        "custom-styles.scss"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "cross_origin_embedder_policy": {
    "value": "require-corp"
  },
  "cross_origin_opener_policy": {
    "value": "same-origin"
  },
  "homepage_url": "https://cobaltlab.tech/",
  "incognito": "split",
  "minimum_chrome_version": "100",
  "offline_enabled": true,
  "omnibox": {
    "keyword": "aString"
  },
  "short_name": "AutoCobaltLab",
  "version_name": "Salami"
}