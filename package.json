{
  "name": "chrome-extension",
  "private": true,
  "version": "0.0.0.6",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "postbuild": "esbuild contentScriptSrc/main.ts --bundle --minify --target=chrome100 --outfile=\"dist/CobaltLab Helper/content-script.js\" && cpx \"contentScriptSrc/custom-styles.css\" \"dist/CobaltLab Helper\"",
    "content-script-build": "esbuild contentScriptSrc/main.ts --bundle --allow-overwrite --minify --sourcemap --target=chrome100 --outfile=\"dist/CobaltLab Helper/content-script.js\" && cpx \"contentScriptSrc/custom-styles.css\" \"dist/CobaltLab Helper\""
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.10",
    "@react-buddy/ide-toolbox": "^2.4.0",
    "@react-buddy/palette-mui": "^5.0.1",
    "axios": "^1.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.17.17",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "cpx": "^1.5.0",
    "esbuild": "^0.19.3",
    "path": "^0.12.7",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vite-plugin-html": "^3.2.0"
  }
}
